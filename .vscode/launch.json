{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Last64 Make Run Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/last64_bloom/build/Last64_Bloom.elf",
      "miDebuggerServerAddress": "localhost:9123",
      "miDebuggerPath": "/opt/libdragon/bin/mips64-elf-gdb",
      "cwd": "${workspaceFolder}/last64_bloom",
      "stopAtEntry": false,
      "externalConsole": false,
      "MIMode": "gdb",
      "preLaunchTask": "make-run",
      "postDebugTask": "kill-ares"
    }
  ]
}
